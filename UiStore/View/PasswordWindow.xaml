<Window x:Class="UiStore.View.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UiStore.View"
        ResizeMode="NoResize"
        mc:Ignorable="d"
        Title="Password" Height="152" Width="300" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="20,20,20,21">
        <TextBlock Text="Mật khẩu:" Margin="0,0,0,5"/>
        <PasswordBox x:Name="PasswordBox" Margin="0,0,0,10" KeyDown="PasswordBox_KeyDown"/>
        <Button Content="Ok" Click="Login_Click" Width="50" HorizontalAlignment="Right" Margin="0,0,5,5"/>
    </StackPanel>
</Window>
